<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="540.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="390.0" prefWidth="600.0">
         <children>
            <HBox fx:id="HEADER" prefHeight="40.0" prefWidth="1192.0" spacing="13.0">
               <children>
                  <ImageView fitHeight="14.0" fitWidth="42.0" opacity="0.9" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../drawable/mi.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="13.0" top="13.0" />
                     </HBox.margin>
                  </ImageView>
                  <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="576.0" spacing="10.0">
                     <children>
                        <ImageView fx:id="IMG_CLOSE" fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <image>
                              <Image url="@../drawable/close.png" />
                           </image>
                           <viewport>
                              <Rectangle2D height="13.0" minY="-1.0" width="11.0" />
                           </viewport>
                           <HBox.margin>
                              <Insets bottom="14.0" top="14.0" />
                           </HBox.margin>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets left="12.0" right="12.0" />
               </padding>
            </HBox>
            <StackPane fx:id="CONTENT" prefHeight="470.0" prefWidth="600.0">
               <children>
                  <WebView fx:id="BROWSER" prefHeight="470.0" prefWidth="600.0" />
               </children>
            </StackPane>
            <Hyperlink fx:id="WHY_LOGIN" alignment="CENTER" prefHeight="30.0" prefWidth="602.0" style="-fx-border-color: transparent;" text="Why login" />
         </children>
      </VBox>
   </children>
</StackPane>
